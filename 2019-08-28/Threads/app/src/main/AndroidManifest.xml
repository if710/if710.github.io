<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufpe.cin.android.threads">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AsyncTaskActivity"
            android:label="AsyncTask" />
        <activity
            android:name=".SemThreads"
            android:label="Sem Threads" />
        <activity
            android:name=".ThreadSimples"
            android:label="Thread Simples" />
        <activity
            android:name=".ThreadRunOnUI"
            android:label="Thread Run On UI" />
        <activity
            android:name=".ThreadViewPost"
            android:label="Thread ViewPost" />
        <activity
            android:name=".ThreadANR"
            android:label="Thread ANR" />
        <activity android:name=".CountdownActivity" />
        <activity android:name=".ListAdapterActivity" />
        <activity android:name=".PostDelayedActivity" />
        <activity android:name=".RecyclerViewActivity"></activity>
    </application>

</manifest>